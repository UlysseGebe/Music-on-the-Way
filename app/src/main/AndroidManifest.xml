<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hetic.musicontheway">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:logo="@drawable/logo"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FireBase.Add.NewEvent"></activity>
        <activity android:name=".recyclerView.EventActivity"></activity>
        <activity android:name=".recyclerView.EventDetailActivity"></activity>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity android:name=".Maps.MapsActivity"></activity>
        <activity android:name=".ActivityPlayerArtistefromUser"></activity>
        <activity android:name=".ActivityPlayerArtisteFromArtiste"></activity>
    </application>

</manifest>